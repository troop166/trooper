[flake8]
max-line-length = 88
extend-ignore = E203,E501
extend-exclude = migrations,venv
per-file-ignores =
    # E501: line too long
    # F403: `from module import *` used; unable to detect undefined names
    # F405: `name` may be undefined, or defined from star imports
    */settings/*.py:E501,F403,F405
